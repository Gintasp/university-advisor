@{
    ViewBag.Title = "Review";
}

<h2 class="mt-50">Leave a review</h2>
<p>Your reviews help fellow students evaluate their future choices.</p>
<ul class="nav nav-tabs mt-20">
    <li class="active">
        <a href="#studyProgramTab" data-toggle="tab">Study Program</a>
    </li>
    <li>
        <a href="#courseTab" data-toggle="tab">Course</a>
    </li>
    <li>
        <a href="#lecturerTab" data-toggle="tab">Lecturer</a>
    </li>
</ul>

<div class="tab-content">
    <div id="studyProgramTab" class="tab-pane active">
        @Html.Partial("Partials/_StudyProgramFormPartial")
    </div>

    <div id="courseTab" class="tab-pane">
        @Html.Partial("Partials/_CourseFormPartial")
    </div>
    <!--LECTURER TAB-->
    <div id="lecturerTab" class="tab-pane">
        @Html.Partial("Partials/_LecturerFormPartial")
    </div>
</div>

@section scripts
{
    @Scripts.Render("~/bundles/review")
}
